{
  "manifest_version": 2,
  "name": "ACMusic",
  "version": "0.1",

  "browser_action": {
    "default_icon": {
      "16": "icons/off-16.png",
      "32": "icons/off-32.png",
      "48": "icons/off-48.png",
      "128": "icons/off-128.png"
    },
    "default_popup": "popup/popup.html"
  },
  "background": {
    "page": "background-page.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/off-16.png",
    "32": "icons/off-32.png",
    "48": "icons/off-48.png",
    "128": "icons/off-128.png"
  }
}
